extends ../layouts/default

block variables
    - const title = "Каталог запчастей MAZDA";

block styles
  link(rel="stylesheet" href="./styles/catalog-tec-doc.css")

block content
  div#page
    include ../../blocks/components/header/mobile_menu
    div.page
      include ../../blocks/modules/header/header
      div.container
        div.bread-crumbs
          a(href='./home.html' class='text_bc bread-crumbs_item') Главная
          a(href='./catalog-tecdoc-step-1.html' class='text_bc bread-crumbs_item') Бренды
          span.text_bc.bread-crumbs_item Модели Mazda
        h1.title_p Каталог запчастей MAZDA
      div.container
        div.catalog_tec_doc_step_2
          include ../../blocks/components/tecdoc/search-name-car
          include ../../blocks/components/tecdoc/years-filters
          include ../../blocks/components/original-catalogs/catalog-cards-car
      include ../../blocks/modules/footer/footer

block scripts
  script(src="js/catalog_tec_doc.js")
  script(src="js/main.js")
  script(src="js/vendor.js")
