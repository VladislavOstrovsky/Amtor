extends ../layouts/default

block variables
    - const title = "Каталог для ТО";

block styles
  link(rel="stylesheet" href="./styles/catalog-to.css")

block content
  div#page
    include ../../blocks/components/header/mobile_menu
    div.page
      include ../../blocks/modules/header/header
      div.container
        div.bread-crumbs
          a(href='./home.html' class='text_bc bread-crumbs_item') Главная
          span.text_bc.bread-crumbs_item Каталог для ТО
        h1.title_p Каталог для ТО
      div.container
        div.catalog_to_wrapper
          include ../../blocks/components/original-catalogs/catalog-marks
      include ../../blocks/modules/footer/footer

block scripts
  script(src="js/catalog_to.js")
  script(src="js/main.js")
  script(src="js/vendor.js")
