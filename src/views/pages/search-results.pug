extends ../layouts/default

block variables
    - const title = "Результат поиска";

block styles
  link(rel="stylesheet" href="./styles/search-results.css")

block content
  div#page
    include ../../blocks/components/header/mobile_menu
    div.page
      include ../../blocks/modules/header/header
      div.container
        div.bread-crumbs
          a(href='./home.html' class='text_bc bread-crumbs_item') Главная
          span.text_bc.bread-crumbs_item Результат поиска
        h1.title_p Результат поиска
      div.container
        div.search_result_wrapper
          include ../../blocks/components/original-catalogs/catalog-table/search-result-table
      include ../../blocks/modules/footer/footer

block scripts
  script(src="js/main.js")
  script(src="js/vendor.js")
