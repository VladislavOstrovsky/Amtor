extends ../layouts/default

block variables
    - const title = "Каталог неоригинальных запчастей";

block styles
  link(rel="stylesheet" href="./styles/catalog-tec-doc.css")

block content
  div#page
    include ../../blocks/components/header/mobile_menu
    div.page
      include ../../blocks/modules/header/header
      div.container
        div.bread-crumbs
          a(href='./home.html' class='text_bc bread-crumbs_item') Главная
          span.text_bc.bread-crumbs_item Каталог неоригинальных запчастей
        h1.title_p Каталог неоригинальных запчастей
      div.container
        div.catalog_tec_doc_wrapper
          include ../../blocks/components/original-catalogs/search-car
          include ../../blocks/components/original-catalogs/catalog-marks
      include ../../blocks/modules/footer/footer

block scripts
  script(src="js/catalog_tec_doc.js")
  script(src="js/main.js")
  script(src="js/vendor.js")
